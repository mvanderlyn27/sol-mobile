{
  "expo": {
    "name": "slice-of-life",
    "slug": "slice-of-life",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "com.supabase",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0B0C06"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sliceoflife.sliceoflife"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon/foreground.png",
        "backgroundImage": "./src/assets/images/adaptive-icon/background.png",
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-font"
      
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ce0eb959-ac1b-41d5-8b08-416d64c88369"
      }
    },
    "owner": "sliceoflife"
  }
}
